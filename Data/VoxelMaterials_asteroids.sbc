            <SimpleTilingSetup>
                <InitialScale>2</InitialScale>
                <ScaleMultiplier>7</ScaleMultiplier>
                <InitialDistance>5</InitialDistance>
                <DistanceMultiplier>5</DistanceMultiplier>

                <Far1Distance>10</Far1Distance>

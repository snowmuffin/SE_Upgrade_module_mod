            <Far1Distance>10</Far1Distance>
            <Far1Scale>10</Far1Scale>

            <Far2Distance>90</Far2Distance>
            <Far2Scale>35</Far2Scale>
            <LandingEffect>Landing_Jet_Ground_Dust</LandingEffect>

        </VoxelMaterial>
